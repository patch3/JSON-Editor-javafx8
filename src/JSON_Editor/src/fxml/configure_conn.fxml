<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="configureConnScene" prefHeight="400.0" prefWidth="290.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="src.controller.ConfigureConn">
    <children>
        <TextField fx:id="hostName" layoutX="44.0" layoutY="55.0" prefHeight="32.0" prefWidth="220.0"
                   promptText="host name"/>
        <TextField fx:id="port" layoutX="44.0" layoutY="109.0" prefHeight="32.0" prefWidth="220.0" promptText="port"/>
        <TextField fx:id="userName" layoutX="44.0" layoutY="161.0" prefHeight="32.0" prefWidth="220.0"
                   promptText="user name"/>
        <TextField fx:id="password" layoutX="44.0" layoutY="212.0" prefHeight="32.0" prefWidth="220.0"
                   promptText="password"/>
        <Button fx:id="saveButton" layoutX="71.0" layoutY="265.0" mnemonicParsing="false" prefHeight="46.0"
                prefWidth="149.0" text="Save"/>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="71.0" layoutY="22.0" prefHeight="18.0"
               prefWidth="149.0" text="Configure"/>
        <Button fx:id="checkButton" layoutX="71.0" layoutY="328.0" mnemonicParsing="false" prefHeight="46.0"
                prefWidth="149.0" text="Check connect"/>
    </children>
</AnchorPane>
