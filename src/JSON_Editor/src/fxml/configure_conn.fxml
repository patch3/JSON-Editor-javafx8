<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="configureConnScene" prefHeight="400.0" prefWidth="290.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="src.controller.ConfigureConn">
    <children>
        <TextField fx:id="hostName" layoutX="44.0" layoutY="56.0" prefHeight="32.0" prefWidth="220.0"
                   promptText="host name"/>
        <TextField fx:id="port" layoutX="44.0" layoutY="113.0" prefHeight="32.0" prefWidth="220.0" promptText="port"/>
        <TextField fx:id="userName" layoutX="44.0" layoutY="168.0" prefHeight="32.0" prefWidth="220.0"
                   promptText="user name"/>
        <TextField fx:id="password" layoutX="44.0" layoutY="227.0" prefHeight="32.0" prefWidth="220.0"
                   promptText="password"/>
        <Button fx:id="saveButton" layoutX="71.0" layoutY="279.0" mnemonicParsing="false" prefHeight="45.0"
                prefWidth="149.0" text="Save"/>
        <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="71.0" layoutY="14.0"
               prefHeight="18.0" prefWidth="149.0" text="Configure"/>
        <Button fx:id="checkButton" layoutX="71.0" layoutY="334.0" mnemonicParsing="false" prefHeight="45.0"
                prefWidth="149.0" text="Check connect"/>
        <Label fx:id="hostNameLabel" layoutX="44.0" layoutY="39.0" text="Host name"/>
        <Label fx:id="portLabel" layoutX="44.0" layoutY="96.0" text="Port"/>
        <Label fx:id="userNameLabel" layoutX="44.0" layoutY="151.0" text="User name"/>
        <Label fx:id="passwordLabel" layoutX="44.0" layoutY="210.0" text="Password"/>
    </children>
</AnchorPane>
